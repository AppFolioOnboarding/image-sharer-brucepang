<%= image_tag(@image.url, class: 'js-image image')  %>
<div class="js-tag-list">
  <div class="js-tag-list">
    <% @image.tag_list.each do |tag| %>
      <%= link_to tag, images_path(tag: tag) %>
    <% end %>
  </div>
</div>
<%= link_to 'Delete',
            image_path(@image),
            method: :delete,
            class: 'js-delete-image',
            data: { confirm: 'Are you sure you want to delete the image?' } %>

